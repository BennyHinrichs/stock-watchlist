{
  "name": "stock-watchlist",
  "version": "0.0.1",
  "homepage": "https://bennyhinrichs.github.io/stock-watchlist/",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && sed -i '' 's/_app/app/g' build/index.html && mv build/_app build/app && cp build/index.html build/404.html && touch build/.nojekyll && npm run prepack",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "predeploy": "pnpm run build",
    "deploy": "gh-pages -d build"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.50.1",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@tailwindcss/vite": "^4.0.0",
    "@types/d3-scale": "^4.0.9",
    "@types/node": "^22.13.8",
    "bits-ui": "0.22.0",
    "clsx": "^2.1.1",
    "cmdk-sv": "^0.0.18",
    "gh-pages": "^6.3.0",
    "lucide-svelte": "^0.475.0",
    "prettier": "^3.5.0",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "publint": "^0.3.2",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwind-merge": "^3.0.1",
    "tailwind-variants": "^0.3.1",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "dependencies": {
    "@layerstack/utils": "^0.1.0",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/svelte-query": "^5.66.0",
    "d3-array": "^3.2.4",
    "d3-scale": "^4.0.2",
    "layerchart": "^0.99.1",
    "tailwindcss": "^4.0.6"
  }
}
